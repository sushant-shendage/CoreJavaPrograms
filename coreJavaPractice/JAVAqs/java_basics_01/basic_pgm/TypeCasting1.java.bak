class TypeCasting1
{
	public static void main(String[] args) 
	{
		byte bytevar=56;

		short shortvar;
		shortvar=bytevar;

		int intvar;
		intvar=bytevar;

		long longvar;
		longvar=bytevar;

		float floatvar;
		floatvar=bytevar;

		double doublevar;
		doublevar=bytevar;

		char charvar;
		charvar=bytevar;
		/*CTE: incompatible types: 
		possible lossy conversion from byte to char*/
		
		boolean booleanvar;
		booleanvar=bytevar;
		//CTE:incompatible types: byte cannot be converted to boolean
		

		System.out.println("byte variable   : "+bytevar);
		System.out.println("byte in short   : "+shortvar);
		System.out.println("byte in int     : "+intvar);
		System.out.println("byte in long    : "+longvar);
		System.out.println("byte in float   : "+floatvar);
		System.out.println("byte in double  : "+doublevar);
		System.out.println("byte in char    : "+charvar); 
		//System.out.println("byte in double  : "+charvar);
		
	}
}
