class ZeroPresentDigit11 
{
	public static void main(String[] args) 
	{
		int num=1234,zero=0;

		System.out.println("Given number: "+num);

		while(num>0)
		{
			int numMod=num%10;

			if(numMod==0)
			{
				zero=1;
			}
			num/=10;
		}

		if (zero==1)
		{
			System.out.println("Result: zero is present in string");
		}
		else
		{
			System.out.println("Result: zero is not present in string");
		}
		
	}
}
