class CheckPrimeNumWithinRangeWWLR04_7thmarch24 
{
	public static void main(String[] args) 
	{
		 int start=1,end=100;

		 while(end>start)
		 {
			 System.out.println("pointer is at while(end<=start)");
			 int num=end;
			 int i=2;

			 while(i<num)
			 {
				 //System.out.println("pointer is at while(i<num)");
				 if(num%i==0)
				 {
				 //System.out.println("mod:"+num%i);
				 //System.out.println("i:"+i);

					 break;
				 }
				 i++;
			 }
			 if(i==num)
			 {
				System.out.println("prime+ : "+num);
			 }
			end--;
		 }

		 //System.out.println("pgm completed");
	}
	//nested while print prime within range reverce direction
}